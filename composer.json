{
    "name" : "lyseontech/doctotext",
    "description" : "Convert doc to text, wrapper to libreoffice",
    "license" : "MIT",
    "type" : "library",
    "homepage" : "https://github.com/lyseontech/doctotext",
    "authors" : [{
            "name" : "Vitor Mattos",
            "homepage" : "https://github.com/vitormattos"
        }
    ],
    "support" : {
        "source" : "https://github.com/librecodecoop/doctotext/tree/master",
        "issues" : "https://github.com/librecodecoop/doctotext/issues"
    },
    "keywords" : [
        "php",
        "php7",
        "libreoffice",
        "libreoffice-writer",
        "wordcount",
